{
  "name": "lunchmove",
  "version": "0.0.0",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "backbone": "~1.1.2",
    "backbone.marionette": "~2.4.1",
    "backbone.select": "~1.3.0",
    "backbone.radio": "~0.9.0",
    "bootstrap-sass-official": "~3.3.0",
    "jquery": "2.1.1",
    "jquery.cookie": "~1.4.1",
    "radio-shim": "https://gist.github.com/7992474cdb1c5672d88b.git",
    "select2": "4.0.0",
    "underscore": "~1.8.3",
    "typeahead.js": "~0.11.1"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "jquery": true,
        "underscore": true
      }
    },
    "backbone.marionette": {
      "dependencies": {
        "backbone": true,
        "backbone.babysitter": true,
        "backbone.radio": true
      }
    },
    "backbone.select": {
      "dependencies": {
        "backbone": true
      }
    },
    "bootstrap-sass-official": {
      "main": "assets/javascripts/bootstrap.js"
    },
    "featherlight": {
      "main": [
        "release/featherlight.min.js",
        "release/featherlight.gallery.min.js"
      ],
      "dependencies": {
        "jquery": true
      }
    },
    "radio-shim": {
      "main": "radio.shim.js",
      "dependencies": {
        "backbone.marionette": true
      }
    },
    "select2": {
      "main": "dist/js/select2.full.js",
      "dependencies": {
        "jquery": true
      }
    }
  },
  "resolutions": {
    "underscore": ">=1.8.3"
  }
}
